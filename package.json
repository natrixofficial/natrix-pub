{
	"name": "natrix-pub",
	"version": "0.0.1",
	"license": "ISC",
	"scripts": {
		"build": "npm run build:all",
		"build:all": "npx nx run-many --target=build --all --parallel",
		"build:affected": "npx nx affected:build --parallel",
		"test": "npm run test:all",
		"test:all": "npx nx run-many --target=test --all --parallel",
		"test:affected": "npx nx affected:test --parallel",
		"lint": "npx nx affected:lint --parallel=5 --verbose",
		"lint:affected": "npx nx affected:lint --parallel=5 --verbose",
		"format": "npx nx format:write",
		"postformat": "npx nx affected:lint --parallel --fix",
		"format:check": "npx nx format:check"
	},
	"private": true,
	"devDependencies": {
		"@nrwl/eslint-plugin-nx": "15.8.9",
		"@nrwl/jest": "15.8.9",
		"@nrwl/js": "15.8.9",
		"@nrwl/linter": "15.8.9",
		"@nrwl/workspace": "15.8.9",
		"@types/bcrypt": "^5.0.0",
		"@types/jest": "29.4.4",
		"@types/node": "18.7.1",
		"@typescript-eslint/eslint-plugin": "5.51.0",
		"@typescript-eslint/parser": "5.51.0",
		"commitlint-config-jira": "^1.5.2",
		"commitlint-plugin-jira-rules": "^1.5.1",
		"eslint": "~8.33.0",
		"eslint-config-prettier": "~8.6.0",
		"eslint-plugin-jest": "~27.2.1",
		"eslint-plugin-unicorn": "~45.0.2",
		"jest": "29.4.3",
		"jest-environment-jsdom": "29.4.3",
		"jest-environment-node": "^29.4.1",
		"nx": "15.8.9",
		"prettier": "2.8.3",
		"ts-jest": "29.0.5",
		"ts-node": "10.9.1",
		"typescript": "4.9.5"
	},
	"dependencies": {
		"bls-eth-wasm": "^1.0.3",
		"bls-wasm": "^0.4.7",
		"jose-node-cjs-runtime": "^4.3.8",
		"reflect-metadata": "^0.1.13",
		"tslib": "^2.3.1"
	}
}
